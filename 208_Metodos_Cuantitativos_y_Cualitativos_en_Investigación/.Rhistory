matrix_T <- function(matriz){
mt <- c()
for (i in seq(1,dim(matriz)[1])) {
mt <- c(mt, matriz[i,])
}
return(matrix(mt, nrow = dim(matriz)[2], ncol = dim(matriz)[1]))
}
